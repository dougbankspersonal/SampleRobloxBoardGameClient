{
  "name": "SRBGC",
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"SRBGCShared": {
				"$path": "src/SRBGCShared"
			},
			"Cryo": {
				"$path": "../cryo/default.project.json"
			},
			"RobloxBoardGameShared": {
				"$path": "../RobloxBoardGame/src/Shared/default.project.json"
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			"SRBGCServer": {
				"$path": "src/SRBGCServer"
			},
			"RobloxBoardGameServer": {
				"$path": "../RobloxBoardGame/src/Server/default.project.json"
			}
		},

		"StarterGui": {
			"$className": "StarterGui",
			"MainScreenGui": {
				"$className": "ScreenGui",
        "SRBGCClient": {
          "$path": "src/SRBGCClient"
        },
        "RobloxBoardGameStarterGui": {
          "$path": "../RobloxBoardGame/src/StarterGui/default.project.json"
        }
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "../RobloxBoardGame/src/StarterPlayerScripts/default.project.json"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.2,
            0.9,
            0.4
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}