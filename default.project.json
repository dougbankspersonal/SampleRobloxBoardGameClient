{
  "name": "SRBGC",
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,

			"SRBGCShared": {
				"$path": "src/SRBGCShared"
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,

			"SRBGCServer": {
				"$path": "src/SRBGCServer"
			}
		},

		"StarterGui": {
			"$className": "StarterGui",
      "$ignoreUnknownInstances": true,

			"MainScreenGui": {
				"$className": "ScreenGui",
        "SRBGCClient": {
          "$path": "src/SRBGCClient"
        }
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$ignoreUnknownInstances": true
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.2,
            0.3,
            0.4
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}